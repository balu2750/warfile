node {  
    stage('checkout') { 
        sh 'cd /var/lib/jenkins/workspace/Temenos-sample/.'
        sh 'rm -rf *'
        sh 'git clone https://github.com/balu2750/warfile'
    }
    stage('build') {
        sh 'mv  /var/lib/jenkins/workspace/Temenos-sample/warfile/* /var/lib/jenkins/workspace/Temenos-sample/' 
        sh 'mvn clean package'     
   }
        
    stage("publish to nexus") {
            
            nexusArtifactUploader artifacts: [[artifactId: 'bitsy-aisp-service-account-access-consents', classifier: 'RELEASE', file: 'target/accounts-consent.jar', type: 'jar']], credentialsId: 'Nexus', groupId: 'com.mav', nexusUrl: '172.16.238.113:8081', nexusVersion: 'nexus3', protocol: 'http', repository: 'bisy-aisp-service-account-access-consents', version: '1.3'
            }
        }
    
    
 

