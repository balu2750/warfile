node {  
    stage('checkout') { 
        sh 'cd /var/lib/jenkins/workspace/job-1/.'
        sh 'rm -rf *'
        sh 'git clone https://github.com/balu2750/warfile'
    }
    stage('build') {
        sh 'mv  /var/lib/jenkins/workspace/job-1/warfile/* /var/lib/jenkins/workspace/job-1/' 
        sh 'mvn clean package'     
    }
    stage('deploy with ansible') {
        sh 'ansible-playbook myweb.yml'
        
        }
       
}
 
